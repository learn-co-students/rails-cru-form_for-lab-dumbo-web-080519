<h1>My edit</h1>
<%= form_for(@song) do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :artist_id %><br>
  <%= f.collection_select :artist_id, @artists, :id, :name %><br>

  <%= f.label :genre_id %><br>
  <%= f.collection_select :genre_id, @genres, :id, :name %><br>

  <%= f.submit %>
<% end %>

<p>My edit</p>
